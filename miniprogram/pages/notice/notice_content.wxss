/* CSS Document */

.banner-section {
    padding: 48px 0;
    margin-bottom: 28px;
    background-image: url(../images/banner-bg.png);
    background-position: center center;
    background-repeat: no-repeat;
}

.banner-tabs {
    width: 63%;
    height: 236px;
    padding: 35px 54px 35px 48px;
    margin: auto;
    background-image: url(../images/banner-tabs-bg.png);
    background-position: center center;
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.banner-tabs .nav-tabs {
    border-bottom: 0;
}

.banner-tabs .nav-tabs li a {
    display: block;
    width: 76px;
    color: #4a4a4a;
    font-size: 16px;
    font-weight: normal;
    line-height: 20px;
    padding: 12px 0;
    margin-right: 35px;
}

.nav-tabs>li.active>a,
.nav-tabs>li.active>a:focus,
.nav-tabs>li.active>a:hover {
    border: 0;
    border-top: 2px solid #298ada;
    border-radius: 0;
    background-color: transparent;
}
.nav-tabs>li>a:focus,
.nav-tabs>li>a:hover{
    background-color: transparent;
    border: 0;
}
.banner-tabs .tab-content {
    margin-top: 5px;
}

.banner-tabs .form-control,
.banner-tabs .btn-default {
    border-color: #298ada;
    border-radius: 0;
    height: 42px;
    line-height: 28px;
}

.banner-tabs .tab-select .btn-default {
    border-right: 0;
    color: #666666;
}

.tab-select .dropdown-toggle,
.tab-select .dropdown-toggle:hover,
.tab-select .dropdown-toggle:focus,
.tab-select .dropdown-toggle:active,
.tab-select.open .dropdown-toggle,
.tab-select.open .dropdown-toggle:hover,
.tab-select.open .dropdown-toggle:focus,
.tab-select.open .dropdown-toggle:active {
    background-color: #ffffff;
    border-color: #298ada;
}

.banner-tabs .input-group-btn .btn.btn-search{
    background-color: #298ada;
}

.banner-tabs .search {
    display: inline-block;
    width: 28px;
    height: 28px;
    background-position: center center;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    vertical-align: middle;
}

.radio-group {
    position: relative;
    margin-top: 15px;
}

.radio-group .radio-inline {
    padding-left: 2px;
}

.radio-inline+.radio-inline {
    margin-left: 20px;
}

.radio-group .moz-radio {
    overflow: hidden;
    height: 22px;
    font-size: 14px;
    font-weight: normal;
    color: #666666;
    line-height: 22px;
    display: inline-block;
    background-image: url(../images/icon-radio.png);
    background-repeat: no-repeat;
    background-position: -98px 5px;
    padding-left: 18px;
}

.radio-group .radio-selected .moz-radio {
    background-position: 0 5px;
}

.high-search {
    float: right;
    height: 22px;
    font-size: 14px;
    font-weight: normal;
    color: #666666;
    line-height: 22px;
    text-decoration: underline;
}

.high-search:hover,
.high-search:focus {
    text-decoration: underline;
}

.tab-caption {
    width: 99%;
    margin-top: 12px;
    font-size: 12px;
    color: #9ca09e;
    font-weight: normal;
    line-height: 18px;
    padding: 0 2px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.bdstudy-img{
    display: inline-block;
    background-image: url(../images/baidu.jpg);
    background-repeat: no-repeat;
