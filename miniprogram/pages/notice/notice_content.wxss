/*sticky-bar*/

.sticky-bar {
    position: fixed;
    bottom: 30%;
    right: 0;
    z-index: 9999;
}

.sticky-bar ul li {
    display: block;
    position: relative;
    width: 58px;
    height: 58px;
    margin-bottom: 1px;
    background-color: #edeff1;
}

.sticky-bar ul li .layer-toggle {
    display: block;
    height: 100%;
    text-align: center;
    vertical-align: middle;
    background-repeat: no-repeat;
    background-position: center center;
}

.sticky-bar ul li.qq .layer-toggle{
    background-image: url(../images/icon-qq.png);
}

.sticky-bar ul li.back-top .layer-toggle{
    background-image: url(../images/back-top.png);
}

.sticky-bar ul li:hover{
    background-color: #298ada;
}

.sticky-bar ul li.qq:hover .layer-toggle{
    background-image: url(../images/icon-qq-hover.png);
}

.sticky-bar ul li.back-top:hover .layer-toggle{
    background-image: url(../images/back-top-hover.png);
}

.sticky-bar .layer {
    display: none;
    position: absolute;
    bottom: 0;
    right: 58px;
    background-color: #EDEFF1;
    text-align: center;
    padding: 5px 10px;
    min-width: 125px;
    min-height: 58px;
}

.sticky-bar .layer img {
    width: 92px;
}

.sticky-bar .layer p.name{
    font-size: 14px;
    line-height: 28px;
    color: #000;
    text-align: center;
    white-space: nowrap;
}
.sticky-bar .layer p {
    font-size: 14px;
    line-height: 20px;
    color: #000;
    vertical-align: middle;
}
